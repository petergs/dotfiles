!Xft.dpi: 96
Xft.antialias: true
!Xft.hinting: true
!Xft.rgba: rgb
!Xft.autohint: false
!Xft.hintstyle: hintslight
!Xft.lcdfilter: lcddefault

URxvt.depth: 32
URxvt.fading: 10

URxvt.scrollBar: off

URxvt.urgentOnBell: True
